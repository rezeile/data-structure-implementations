// stacktest.cpp: use templated stack
#include <iostream>
#include "stack.h"

// Execution CC -o stack stack.cpp stacktest.cpp

int main() {
   Stack<char, 10> s; // 10 chars
   char ch;
   while ((ch = std::cin.get()) != '\n')
      if (!s.full()) s.push(ch);
   while (!s.empty())
      std::cout << s.pop();
   std::cout << std::endl;
   Stack<double, 4> ds; // 4 doubles
   double d[] =
      {1.0, 3.0, 5.0, 7.0, 9.0, 0.0};
   int i = 0;
   while (d[i] != 0.0 && !ds.full())
      if (!ds.full()) ds.push(d[i++]);
   while (!ds.empty())
      std::cout << ds.pop() << "  ";
   std::cout << std::endl;
   return 0;
}